on property:persist.sys.mtbf.test=1
	write /sys/kernel/debug/cms/debug "LG4"
on property:sys.vivo.disable_charge=*
	write /sys/class/power_supply/cms/disable_charge ${sys.vivo.disable_charge}

on charger
	# VIVO comment: /system already mount, no need below, if add it again, easy cause the error: "Too many symbolic links encountered"
