on property:sys.init_log_level=*
    loglevel ${sys.init_log_level}

# vivo_chg modify start for charging
on property:persist.vivo.enable_equalizing_charge=*
	write /sys/class/power_supply/cms/switch_state ${persist.vivo.enable_equalizing_charge}

on property:persist.vivo.calling_state=*
	write /sys/class/power_supply/cms/calling_state ${persist.vivo.calling_state}

on property:persist.vendor.camera.camera4k_state=*
	write /sys/class/power_supply/cms/camera4k_state ${persist.vendor.camera.camera4k_state}

on property:persist.vendor.camera.charge.close=*
	write /sys/class/power_supply/mcu/camera_charge_close ${persist.vendor.camera.charge.close}

on property:persist.vivo.weixin_calling_state=*
	  write /sys/class/power_supply/cms/weixin_calling_state ${persist.vivo.weixin_calling_state}

on property:persist.vivo.camera_state=*
